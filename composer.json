{
    "name": "stfalcon.com",
    "description": "Portfolio of the studio Stfalcon.com",
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.3.*",
        "doctrine/orm": "2.4.*",
        "doctrine/doctrine-bundle": "1.2.*",
        "twig/extensions": "1.0.*",
        "symfony/assetic-bundle": "2.8.*",
        "kriswallsmith/assetic": "1.3.*",
        "symfony/swiftmailer-bundle": "2.3.*",
        "symfony/monolog-bundle": "2.3.*",
        "sensio/distribution-bundle": "2.3.*",
        "sensio/framework-extra-bundle": "2.3.*",
        "sensio/generator-bundle": "2.3.*",
        "jms/security-extra-bundle": "1.5.*",
        "friendsofsymfony/user-bundle": "1.3.*",
        "doctrine/doctrine-fixtures-bundle": "2.2.*",
        "doctrine/doctrine-migrations-bundle": "1.1.*",
        "gedmo/doctrine-extensions": "2.3.*",
        "stof/doctrine-extensions-bundle": "1.2.*",
        "jms/i18n-routing-bundle": "2.0.*",
        "jms/translation-bundle": "1.2.*",
        "a2lix/translation-form-bundle": "1.3.*",
        "sonata-project/doctrine-orm-admin-bundle": "2.2.*",
        "sonata-project/admin-bundle": "2.2.*",
        "sonata-project/cache-bundle": "2.1.*",
        "sonata-project/block-bundle": "2.2.*",
        "sonata-project/seo-bundle": "2.0.*",
        "avalanche123/imagine-bundle": "dev-master#eb03645887f732410127a2b3d992befe0f175277",
        "knplabs/knp-paginator-bundle": "2.3.*",
        "knplabs/knp-components": "1.2.*",
        "liip/functional-test-bundle": "1.0.*",
        "vich/uploader-bundle": "1.0.*",
        "sonata-project/user-bundle": "2.2.*",
        "sonata-project/media-bundle": "2.2.*",
        "zendframework/zend-feed": "2.3.*",
        "zendframework/zend-servicemanager": "2.3.*",
        "geshi/geshi": "dev-master",
        "fresh/doctrine-enum-bundle": "2.5",
        "geoip2/geoip2": "2.1.*",
        "ahmedsamy/hype-mailchimp-bundle": "1.2.*",
        "mlehner/gelf-php": "1.0.*",
        "friendsofsymfony/jsrouting-bundle": "1.6.*",
        "iakumai/sphinxsearch-bundle": "0.2.*",
        "sensio/buzz-bundle": "1.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "npm install",
            "grunt less",
            "grunt autoprefixer",
            "grunt csso"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    }
}
