{
    "name": "stfalcon.com",
    "description": "Engine for portfolio of web-studio stfalcon.com",
    "require": {
        "php": ">=5.3.3 <5.5",
        "symfony/symfony": "2.8.*",
        "symfony/assetic-bundle": "2.8.*",
        "symfony/swiftmailer-bundle": "2.3.*",
        "symfony/monolog-bundle": "2.3.*",
        "sensio/distribution-bundle": "4.0.*",
        "sensio/framework-extra-bundle": "2.3.*",
        "sensio/generator-bundle": "2.3.*",
        "sensio/buzz-bundle": "^1.0",
        "twig/extensions": "1.3.*",
        "doctrine/orm": "2.4.7",
        "doctrine/doctrine-bundle": "1.6.*",
        "doctrine/doctrine-fixtures-bundle": "2.2.0",
        "doctrine/doctrine-migrations-bundle": "1.1.*",
        "gedmo/doctrine-extensions": "2.3.9",
        "stof/doctrine-extensions-bundle": "1.2.*",
        "fresh/doctrine-enum-bundle": "2.5",
        "friendsofsymfony/user-bundle": "1.3.*",
        "friendsofsymfony/jsrouting-bundle": "1.6.*",
        "jms/security-extra-bundle": "1.6.*",
        "jms/i18n-routing-bundle": "1.1.1",
        "jms/translation-bundle": "1.1.0",
        "sonata-project/doctrine-orm-admin-bundle": "2.3.*",
        "sonata-project/admin-bundle": "2.3.*",
        "sonata-project/cache-bundle": "2.2.*",
        "sonata-project/block-bundle": "2.2.*",
        "sonata-project/seo-bundle": "2.0.0",
        "sonata-project/user-bundle": "2.2.*",
        "sonata-project/media-bundle": "2.2.*",
        "knplabs/knp-paginator-bundle": "2.5.*",
        "a2lix/translation-form-bundle": "1.3",
        "avalanche123/imagine-bundle": "dev-master#ded1ffdb5865ff681d8a8e8e6a58d1ad55be3043",
        "liip/functional-test-bundle": "1.0.1",
        "vich/uploader-bundle": "0.8.1",
        "ahmedsamy/hype-mailchimp-bundle": "1.2",
        "iakumai/sphinxsearch-bundle": "0.2.*",
        "knplabs/knp-components": "1.3.*",
        "kriswallsmith/assetic": "1.3.*",
        "zendframework/zend-feed": "2.3.*",
        "zendframework/zend-servicemanager": "2.3.*",
        "geshi/geshi": "dev-master",
        "geoip2/geoip2": "2.1.1",
        "mlehner/gelf-php": "v1.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "npm install",
            "grunt less",
            "grunt autoprefixer",
            "grunt csso"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
        ]
    },
    "autoload": {
        "psr-4": { "": "src/" },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "config": {
        "bin-dir": "bin",
        "optimize-autoloader": true,
        "platform": {
            "php": "5.4"
        }
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    }
}
